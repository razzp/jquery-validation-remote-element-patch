{
    "name": "jquery-validation-remote-element-patch",
    "version": "0.0.0",
    "description": "A quick and dirty patch for enabling elements that are outside <form> elements, but linked via the form HTML attribute, to still trigger JQuery Validation.",
    "keywords": [
        "jquery",
        "validation",
        "validate",
        "patch",
        "remote",
        "outside",
        "form",
        "element",
        "elements"
    ],
    "author": "Robert Wells",
    "license": "MIT",
    "bugs": "https://github.com/razzp/jquery-validation-remote-element-patch/issues",
    "homepage": "https://github.com/razzp/jquery-validation-remote-element-patch#readme",
    "repository": "github:razzp/jquery-validation-remote-element-patch",
    "type": "module",
    "main": "./dist/jquery-validation-remote-element-patch.cjs",
    "module": "./dist/jquery-validation-remote-element-patch.js",
    "exports": {
        ".": {
            "import": "./dist/jquery-validation-remote-element-patch.js",
            "require": "./dist/jquery-validation-remote-element-patch.cjs"
        }
    },
    "types": "./dist/jquery-validation-remote-element-patch.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npm run biome && rimraf dist && rollup --config && npm run types",
        "types": "rimraf types && tsc --project tsconfig.types.json && api-extractor run --local --verbose",
        "biome": "biome check --write",
        "release": "release-it"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.6",
        "@microsoft/api-extractor": "^7.55.0",
        "@rollup/plugin-typescript": "^12.3.0",
        "@types/jquery": "^3.5.33",
        "@types/jquery.validation": "^1.17.0",
        "release-it": "^19.0.6",
        "rimraf": "^6.1.0",
        "rollup": "^4.53.2",
        "typescript": "~5.9.3"
    },
    "dependencies": {
        "tslib": "^2.8.1"
    }
}
